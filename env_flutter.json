  "host": "106.12.150.249:7300",
  "basePath": "/mock/5f5b17df115ae3876870790c/examle",
  "tags": [
    {
      "name": "Air",
      "description": "空气模块"
    },
    {
      "name": "Frame",
      "description": "框架功能"
    }
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/Frame/UserLoginApp": {
      "post": {
        "tags": [
          "Frame"
        ],
        "consumes": [
          "application/x-www-form-urlencoded"
        ],
        "produces": [
          "html/text"
        ],
        "summary": "登录",
        "parameters": [
          {
            "name": "LoginId",
            "in": "formData",
            "type": "string",
            "required": true
          },
          {
            "name": "Password",
            "in": "formData",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/LoginResponse"
              }
            }
          }
        }
      }
    },
    "/Air/APPRealData": {
      "post": {
        "tags": [
          "Air"
        ],
        "produces": [
          "html/text"
        ],
        "summary": "实时数据",
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "description": "用户名",
                  "items": {
                    "$ref": "#/definitions/RealDataResponse"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/Air/AppVersion": {
      "post": {
        "tags": [
          "Air"
        ],
        "produces": [
          "html/text"
        ],
        "summary": "app版本信息",
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "Version": {
                  "$ref": "#/definitions/AppVersionResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "definitions": {
    "LoginResponse": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string",
          "description": "用户名"
        },
        "userguid": {
          "type": "string",
          "description": "guid"
        }
      }
    },
    "RealDataResponse": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "example": "6831008A"
        },
        "name": {
          "type": "string",
          "example": "五接站"
        },
        "aqi": {
          "type": "string",
          "example": "63"
        },
        "time": {
          "type": "string",
          "format": "date-time"
        },
        "mainPollutes": {
          "type": "array",
          "description": "主要污染物",
          "items": {
            "$ref": "#/definitions/FactorModel"
          }
        },
        "factorValues": {
          "type": "array",
          "description": "六因子值",
          "items": {
            "$ref": "#/definitions/FactorModel"
          }
        }
      }
    },
    "AppVersionResponse": {
      "type": "object",
      "properties": {
        "verName": {
          "type": "string",
          "example": "1.2.4",
          "description": "版本号"
        },
        "verCode": {
          "type": "string",
          "description": "版本Code",
          "example": "24"
        },
        "verDesc": {
          "type": "string",
          "description": "APP更新信息",
          "example": "欢迎使用！"
        },
        "verPath": {
          "type": "string",
          "description": "APK下载路径",
          "example": "http://223.113.9.184:8899/EQMSPortalTZ/EnvironmentTZ.apk"
        }
      }
    },
    "FactorModel": {
      "type": "object",
      "properties": {
        "factor": {
          "type": "string",
          "example": "PM2.5"
        },
        "value": {
          "type": "string",
          "example": "45"
        },
        "unit": {
          "type": "string",
          "example": "μg/m³"
        }
      }
    }
  }
}
